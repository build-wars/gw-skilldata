{
	"name": "buildwars/gw-skilldata",
	"description": "Guild Wars skill data",
	"license": "MIT",
	"type": "library",
	"keywords": ["guild wars"],
	"authors": [
		{
			"name": "smiley",
			"email": "smiley@chillerlan.net",
			"homepage": "https://github.com/codemasher"
		},
		{
			"name": "Contributors",
			"homepage":"https://github.com/build-wars/gw-skilldata/graphs/contributors"
		}
	],
	"funding": [
		{
			"type": "Ko-Fi",
			"url": "https://ko-fi.com/codemasher"
		}
	],
	"homepage": "https://github.com/build-wars/gw-skilldata",
	"support": {
		"issues": "https://github.com/build-wars/gw-skilldata/issues",
		"source": "https://github.com/build-wars/gw-skilldata"
	},
	"minimum-stability": "stable",
	"prefer-stable": true,
	"require": {
		"php": "^8.1",
		"ext-mbstring": "*"
	},
	"require-dev": {
		"chillerlan/php-httpinterface": "^6.0",
		"monolog/monolog": "^3.6",
		"phan/phan": "^5.4",
		"phpunit/phpunit": "^10.5",
		"phpmd/phpmd": "^2.15",
		"squizlabs/php_codesniffer": "^3.10"
	},
	"suggest": {
		"buildwars/gw-templates": "A en/decoder for Guild Wars templates"
	},
	"autoload": {
		"psr-4": {
			"Buildwars\\GWSkillData\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Buildwars\\GWSkillDataTest\\": "tests",
			"Buildwars\\GWSkillDataTools\\": "tools"
		}
	},
	"scripts": {
		"phan": "@php vendor/bin/phan",
		"phpcs": "@php vendor/bin/phpcs",
		"phpunit": "@php vendor/bin/phpunit"
	},
	"config": {
		"lock": false,
		"sort-packages": true,
		"platform-check": true
	}
}
